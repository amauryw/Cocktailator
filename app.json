{
  "expo": {
    "name": "Cockailator",
    "slug": "Cockailator",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.coloc.cocktailator",
      "versionCode": 3
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.coloc.cocktailator",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    }
  }
}
